<Window x:Class="AgendaWPF.SelecBase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Seleccinar base de datos" Height="294" Width="238">
    <Grid>
        <Button x:Name="but_Continue" Content="Continuar" HorizontalAlignment="Left" Margin="57,189,0,0" VerticalAlignment="Top" Width="120" Height="51" Click="but_Continue_Click"/>
        <ComboBox x:Name="comboBox_BaseDatos" HorizontalAlignment="Left" Margin="57,150,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="textBaseDatos" Content="Base de datos" HorizontalAlignment="Left" Margin="57,21,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.4,0.636" Width="120"/>
        <Label Content="Nueva" HorizontalAlignment="Left" Margin="57,52,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="text_Nueva" HorizontalAlignment="Left" Height="23" Margin="57,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="O una existente" HorizontalAlignment="Left" Margin="57,119,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.768,1.331" Width="120"/>

    </Grid>
</Window>
