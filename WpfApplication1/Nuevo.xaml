<Window x:Class="AgendaWPF.NuevoContacto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Nuevo contacto" Height="298" Width="217">
    <Grid>
        <TextBox x:Name="text_Nombre" HorizontalAlignment="Left" Height="23" Margin="30,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="145"/>
        <TextBox x:Name="text_Telefono" HorizontalAlignment="Left" Height="23" Margin="30,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="145"/>
        <Button x:Name="but_AddContacto" Content="Añadir" HorizontalAlignment="Left" Margin="30,220,0,0" VerticalAlignment="Top" Width="145" RenderTransformOrigin="-0.061,0.451" Height="24" Click="but_AddContacto_Click"/>
        <DatePicker x:Name="date_Fecha" HorizontalAlignment="Left" Margin="30,178,0,0" VerticalAlignment="Top" Text="cumpleaños"/>
        <Label Content="Nombre" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="1.868,1.192" Margin="30,10,0,0"/>
        <Label Content="Telefono" HorizontalAlignment="Left" Margin="30,64,0,0" VerticalAlignment="Top"/>
        <Label Content="Cumpleaños" HorizontalAlignment="Left" Margin="30,138,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
